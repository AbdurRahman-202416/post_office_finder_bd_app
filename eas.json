{
    "cli": {
        "version": ">= 13.0.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "env": {
                "NODE_ENV": "development"
            }
        },
        "preview": {
            "distribution": "internal",
            "env": {
                "NODE_ENV": "production"
            },
            "android": {
                "buildType": "apk"
            }
        },
        "production": {
            "env": {
                "NODE_ENV": "production"
            },
            "android": {
                "buildType": "app-bundle"
            },
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "production": {}
    }
}